<%- include('includes/head.ejs') %>
<link rel="stylesheet" href="/css/product.css">
</head>


<body>
    <%- include('includes/navigation.ejs') %>

    <main>
        <% if (prods.length>0) { %>
        <h1>My Products</h1>
        <p>List of all the products</p>
        <% for (let product of prods) { %>
        <li><%= product.title %></li>
        <% } %>
        <% } else{ %>
        <h1>No Shop Page</h1>
        <% } %>
    </main>
    <%- include('includes/end.ejs') %>